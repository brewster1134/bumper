  "main": "./lib/bumper",
  "name": "bumper",
  "repository": "github:brewster1134/bumper",
  "version": "2.0.0",
  "bin": {
    "bumper": "./bin/bumper"
  },
  "directories": {
    "bin": "./bin",
    "doc": "./doc",
    "lib": "./lib",
    "test": "./test"
  },
  "engines": {
    "node": ">=10"
  },
  "keywords": [
    "api",
    "build",
    "demo",
    "deploy",
    "documentation",
    "frontend",
    "release",
    "styleguide",
    "test"
  ],
  "scripts": {
    "test": "nyc --nycrc-path=./test/.nycrc.json --reporter=text --reporter=html mocha --opts=./test/.mocharc",
    "watch": "mocha --opts=./test/.mocharc --watch-extensions=coffee --watch",
    "watch-cover": "nodemon --exec nyc --nycrc-path=./test/.nycrc.json --reporter=text --reporter=html mocha --opts=./test/.mocharc",
    "ci-test": "nyc --nycrc-path=./test/.nycrc.json --reporter=text-summary mocha --opts=./test/.mocharc",
    "ci-coverage": "nyc --nycrc-path=./test/.nycrc.json report --reporter=text-lcov | coveralls"
  },
  "dependencies": {
    "@babel/core": "^7",
    "babel-loader": "^8",
    "body-parser": "^1",
    "chai": "^4",
    "chalk": "^2",
    "coffee-loader": "^0",
    "coffeescript": "^2",
    "consolidate": "^0",
    "css-loader": "^2",
    "downloads-folder": "^1",
    "express": "^4",
    "fs-extra": "^7",
    "js-yaml": "^3",
    "marked": "^0",
    "mini-css-extract-plugin": "^0",
    "mocha": "^6",
    "node-sass": "^4",
    "nodemon": "^1",
    "normalize.css": "^8",
    "pug": "^2",
    "pug-loader": "^2",
    "sass-loader": "^7",
    "sinon": "^7",
    "sinon-chai": "^3",
    "style-loader": "^0",
    "webpack": "^4",
    "webpack-dev-middleware": "^3",
    "webpack-glob-entry": "^2",
    "webpack-node-externals": "^1",
    "write-file-webpack-plugin": "^4",
    "yargs": "^13",
    "zip-webpack-plugin": "^3"
  },
  "devDependencies": {
    "coffee-coverage": "^3",
    "coveralls": "^3",
    "nyc": "^14"
  }
}
